{
  "buildCommand": "cd apps/receptionist-dashboard && bun run build",
  "outputDirectory": "apps/receptionist-dashboard/.next",
  "installCommand": "bun install",
  "framework": null,
  "crons": [
    {
      "path": "/api/cron/campaigns",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/reminders",
      "schedule": "*/15 * * * *"
    }
  ]
}
