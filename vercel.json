{
  "buildCommand": "bun run build --filter=receptionist-dashboard... --filter=gym-landing...",
  "outputDirectory": ".next",
  "installCommand": "bun install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron?task=sync-checkins",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron?task=send-reminders",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron?task=process-campaigns",
      "schedule": "0 9 * * *"
    }
  ]
}
