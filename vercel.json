{
  "buildCommand": "npm run build --workspace=apps/receptionist-dashboard",
  "installCommand": "npm install",
  "outputDirectory": "apps/receptionist-dashboard/.next",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/campaigns",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/reminders",
      "schedule": "*/15 * * * *"
    }
  ]
}
